{
  "run": [{
    "method": "local.set",
    "params": {
      "name": "{{input.name}}"
    }
  }, {
    "method": "input",
    "params": {
      "title": "Download",
      "form": [{
        "key": "name",
        "title": "model name (discover at https://ollama.ai/library)"
      }]
    }
  }, {
    "method": "shell.run",
    "params": {
      "message": "ollama pull {{local.name}}"
    }
  }, {
    "method": "input",
    "params": { "title": "Download Finished", "description": "Go back to the dashboard and launch the app!" }
  }]
}
